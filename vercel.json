{
  "version": 2,
  "builds": [
    {
      "src": "Ecomm/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxlambdasize": "15mb", "runtime": "python3.10" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
        "src": "/static/(.*)",
        "dest": "/static/$1"
    },
    {
        "src": "/(.*)",
        "dest": "Ecomm/wsgi.py"
    }
  ]
}
